div.outer-wrapper
  div.login-wrapper

    div.register
      div.register-inner
        h1 register
        form(ng-submit="signup($event)")
          input(type="email" placeholder="Email Address" ng-model="signupForm.username")
          input(type="password" placeholder="Password" ng-model="signupForm.password")
          input(type="submit" class="btn btn-purple" value="Register")
